firewall {
    ipv6-name WAN_IN_v6 {
        default-action drop
        description "WAN to LAN (IPv6)"
        rule 10 {
            action accept
            description ICMPv6
            log disable
            protocol icmpv6
        }
        rule 20 {
            action accept
            description "Allow established"
            log disable
            protocol all
            state {
                established enable
                related enable
            }
        }
        rule 21 {
            action drop
            description "Drop invalid"
            log disable
            protocol all
            state {
                invalid enable
            }
        }
        rule 31 {
            action accept
            description "Pegasus TCP allowed"
            destination {
                address 2607:f598:b2c2:fc01:250:c2ff:fe28:3070
                port 22,80,443
            }
            protocol tcp
            state {
                new enable
            }
        }
        rule 32 {
            action accept
            description "Allow MOSH inbound"
            destination {
                port 60000-61000
            }
            protocol udp
        }
    }
    ipv6-name WAN_LOCAL_v6 {
        default-action drop
        description "WAN to router (IPv6)"
        rule 10 {
            action accept
            description ICMPv6
            log disable
            protocol icmpv6
        }
        rule 20 {
            action accept
            description "Allow established"
            log disable
            protocol all
            state {
                established enable
                related enable
            }
        }
        rule 21 {
            action drop
            description "Drop invalid"
            log disable
            protocol all
            state {
                invalid enable
            }
        }
        rule 30 {
            action accept
            description DHCPv6
            destination {
                port 546,547
            }
            log disable
            protocol udp
        }
        rule 31 {
            action accept
            description "Cycletrailer TCP allowed"
            destination {
                port 22
            }
            protocol tcp
            state {
                new enable
            }
        }
    }
}
interfaces {
    ethernet eth0 {
        address 10.20.0.1/16
        address fd95:0c38:25f0::1/64
        description "NB LAN"
        duplex auto
        speed auto
    }
    ethernet eth1 {
        duplex auto
        speed auto
    }
    ethernet eth2 {
        address 192.195.83.130/29
        address 192.195.83.134/29
        address 192.195.83.133/29
        address 10.19.0.1/24
        address fd95:0c38:25f0:1::1/64
        description "MonkeyBrains WAN"
        dhcpv6-pd {
            no-dns
            pd 1 {
                interface eth0 {
                    host-address ::1
                    no-dns
                    prefix-id :1
                    service dhcpv6-stateless
                }
                prefix-length 56
            }
            rapid-commit enable
        }
        duplex auto
        firewall {
            in {
                ipv6-name WAN_IN_v6
            }
            local {
                ipv6-name WAN_LOCAL_v6
            }
        }
        ip {
            enable-proxy-arp
        }
        ipv6 {
            address {
                autoconf
            }
        }
        speed auto
    }
    ethernet eth3 {
        duplex auto
        speed auto
    }
}
port-forward {
    auto-firewall enable
    hairpin-nat enable
    lan-interface eth0
    wan-interface eth2
}
protocols {
    static {
        route 0.0.0.0/0 {
            next-hop 192.195.83.129 {
            }
